{
  "name": "@kkkqkx123/kodex",
  "version": "1.0.03",
  "bin": {
    "kode": "cli.js",
    "kwa": "cli.js",
    "kd": "cli.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "cli.js",
  "author": "kkkqkx123",
  "license": "Apache-2.0",
  "description": "AI-powered terminal assistant that understands your codebase, edits files, runs commands, and automates development workflows.",
  "homepage": "https://github.com/shareAI-lab/kode",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shareAI-lab/kode.git"
  },
  "bugs": {
    "url": "https://github.com/shareAI-lab/kode/issues"
  },
  "files": [
    "cli.js",
    "yoga.wasm",
    "src/**/*",
    "scripts/postinstall.js",
    ".npmrc"
  ],
  "scripts": {
    "dev": "bun run ./src/entrypoints/cli.tsx --verbose",
    "build": "bun run scripts/build.ts",
    "clean": "rm -rf cli.js",
    "prepublishOnly": "bun run build && node scripts/prepublish-check.js",
    "postinstall": "node scripts/postinstall.js || true",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "test": "bun test",
    "test:cleanup": "node test-cleanup-simple.js",
    "typecheck": "tsc --noEmit"
  },
  "optionalDependencies": {
    "@img/sharp-darwin-arm64": "0.34.3",
    "@img/sharp-linux-arm": "0.34.3",
    "@img/sharp-linux-x64": "0.34.3",
    "@img/sharp-win32-x64": "0.34.3"
  },
  "dependencies": {
    "@anthropic-ai/bedrock-sdk": "0.24.0",
    "@anthropic-ai/sdk": "0.61.0",
    "@anthropic-ai/vertex-sdk": "0.13.1",
    "@commander-js/extra-typings": "^14.0.0",
    "@inkjs/ui": "2.0.0",
    "@modelcontextprotocol/sdk": "1.17.5",
    "@types/lodash-es": "4.17.12",
    "@types/react": "^19.1.12",
    "ansi-escapes": "7.0.0",
    "chalk": "^5.6.0",
    "cli-highlight": "2.1.11",
    "cli-table3": "0.6.5",
    "commander": "^14.0.0",
    "debug": "4.4.1",
    "diff": "^8.0.2",
    "dotenv": "^17.2.2",
    "env-paths": "^3.0.0",
    "figures": "6.1.0",
    "glob": "11.0.3",
    "gray-matter": "4.0.3",
    "highlight.js": "11.11.1",
    "ink": "6.2.3",
    "ink-link": "4.1.0",
    "ink-select-input": "6.2.0",
    "ink-text-input": "^6.0.0",
    "lodash-es": "^4.17.21",
    "lru-cache": "11.2.1",
    "marked": "16.2.1",
    "nanoid": "^5.1.5",
    "node-fetch": "3.3.2",
    "node-html-parser": "7.0.1",
    "openai": "^5.19.1",
    "react": "19.1.1",
    "semver": "^7.7.2",
    "shell-quote": "1.8.3",
    "spawn-rx": "^5.1.2",
    "tsx": "4.20.5",
    "turndown": "^7.2.1",
    "undici": "7.15.0",
    "wrap-ansi": "9.0.0",
    "zod": "4.1.5",
    "zod-to-json-schema": "3.24.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@types/bun": "latest",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "bun-types": "1.2.21",
    "ink-testing-library": "^4.0.0",
    "jest": "^30.1.3",
    "prettier": "3.6.2",
    "typescript": "5.9.2"
  },
  "overrides": {
    "string-width": "^8.1.0",
    "strip-ansi": "^7.1.0"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "keywords": [
    "cli",
    "ai",
    "assistant",
    "agent",
    "kode",
    "shareai",
    "terminal",
    "command-line"
  ]
}